  id        Int      @id @default(autoincrement())
  namePan  String? @db.VarChar(256)
  nameAadhar  String? @db.VarChar(256)
  nameCml  String? @db.VarChar(256)
  phone  String? @db.VarChar(256)
  email  String? @db.VarChar(256)
